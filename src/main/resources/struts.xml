<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="iwork" extends="struts-default">

		<action name="loginUser" class="loginAction">
			<result name="success" type="redirect">listUser.action</result>
		</action>

		<action name="logoutUser" class="loginAction">
			<result name="success">login.jsp</result>
		</action>

		<action name="update" class="userAction">
			<result name="success">/update.jsp</result>
		</action>

		<action name="update" class="updateAction">
			<result name="success" type="redirect">listUser.action</result>
		</action>

		<action name="listUser" class="userAction">
			<result name="success">/listUser.jsp</result>
		</action>

	</package>
</struts>
